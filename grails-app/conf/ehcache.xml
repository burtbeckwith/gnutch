<ehcache>
    <defaultCache
            maxElementsInMemory="50"
            eternal="false"
            timeToIdleSeconds="20"
            timeToLiveSeconds="20"
            overflowToDisk="false"
            diskPersistent="false"
            memoryStoreEvictionPolicy="LRU"
            />


    <cache name="processedUrlCache"
           maxElementsInMemory="50000"
           eternal="true">
    </cache>
</ehcache>
